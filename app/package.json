{
  "name": "@ets/react-boilerplate",
  "version": "0.0.1",
  "description": "React boilerplate setup",
  "main": "server/index.js",
  "scripts": {
    "test": "npm run test:server && npm run test:client",
    "build": "npm-run-all build:js build:css build:assets build:ping",
    "build:assets": "test -e public && rsync -av public/ dist",
    "build:css": "./node_modules/.bin/node-sass --output-style=compressed sass/main.scss dist/css/styles.css",
    "build:js": "./node_modules/.bin/webpack --progress --colors",
    "build:ping": "rsync -a server/ping.json dist/",
    "prebuild": "mkdir -p dist/{js,css,img} ./{logs,public}",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:guahanweb/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": {
    "name": "Garth Henson",
    "email": "garth@guahanweb.com"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-plugin-react": "^7.3.0",
    "jsdom": "^11.2.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "sinon": "^3.2.1",
    "supertest": "^3.0.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "boom": "^5.2.0",
    "hapi": "^16.5.2",
    "hapi-and-healthy": "^5.0.0",
    "inert": "^4.2.1",
    "moment": "^2.18.1",
    "newrelic": "^2.2.0",
    "pug": "^2.0.0-rc.3",
    "request": "^2.81.0",
    "vision": "^4.1.1",
    "winston": "^2.3.1"
  }
}
